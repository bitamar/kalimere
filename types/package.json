{
  "name": "@kalimere/types",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["src/*.ts"]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
