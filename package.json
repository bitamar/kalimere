{
  "name": "kalimere",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "format": "prettier \"api/**/*.{js,jsx,ts,tsx,json,md,css,scss,html,yml,yaml}\" \"front/**/*.{js,jsx,ts,tsx,json,md,css,scss,html,yml,yaml}\" --write",
    "format:check": "prettier \"api/**/*.{js,jsx,ts,tsx,json,md,css,scss,html,yml,yaml}\" \"front/**/*.{js,jsx,ts,tsx,json,md,css,scss,html,yml,yaml}\" --check",
    "check": "npm run format:check && npm --prefix api run lint && npm --prefix api run type-check && npm --prefix api test && npm --prefix front run lint && npm --prefix front run type-check && npm --prefix front test"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@fastify/formbody": "8.0.2"
  }
}
